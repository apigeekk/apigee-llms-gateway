<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-TooManyRequests">
  <DisplayName>RF-TooManyRequests</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <Headers/>
      <Payload contentType="text/plain"/>
      <StatusCode>429</StatusCode>
      <ReasonPhrase>Too many requests. Consider making fewer requests per minute.</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
